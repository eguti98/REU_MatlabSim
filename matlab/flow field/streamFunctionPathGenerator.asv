%stream function path planning

[X,Y] = meshgrid(0:.1:10 , 0:.1:10);


%beginning source
[u1,v1,psi1] = sourceFlow(0,0,5,X,Y);

%end sink
[u2,v2,psi2] = sourceFlow(10,10,-5,X,Y);

%upper Left sink
[u3,v3,psi3] = sourceFlow(0,10,-5,X,Y);

%lower Right sink
[u4,v4,psi4] = sourceFlow(10,0,-5,X,Y);


U =u1+u2+u3;
V = v1+v2+v3;
PSI = psi1+psi2+psi3;

figure()
contour(X,Y,PSI, -5:.2:5);  %0:.2:5
hold on;

